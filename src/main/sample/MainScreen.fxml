                                        AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="75.0"/>
                                <Button fx:id="button_product_add" layoutX="198.0" layoutY="379.0"
                                        mnemonicParsing="false" onAction="#actionAddProduct" text="Add"
                                        AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="147.0"/>
                            </children>
                        </AnchorPane>
                    </items>
                </SplitPane>
            </children>
        </AnchorPane>
        <Button fx:id="button_exit" layoutX="711.0" layoutY="539.0" mnemonicParsing="false" onAction="#actionExit"
                text="Exit" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0"/>
    </children>
</AnchorPane>
